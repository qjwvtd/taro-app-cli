{
  "name": "taro-app-cli",
  "version": "1.0.0300",
  "private": true,
  "description": "--",
  "templateInfo": {
    "name": "mobx",
    "typescript": false,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "npm run lint && cross-env NODE_ENV=production && taro build --type weapp",
    "build:swan": "npm run lint && cross-env NODE_ENV=production && taro build --type swan",
    "build:alipay": "npm run lint && cross-env NODE_ENV=production && taro build --type alipay",
    "build:tt": "npm run lint && cross-env NODE_ENV=production && taro build --type tt",
    "build:h5": "npm run lint && cross-env NODE_ENV=production && taro build --type h5",
    "build:rn": "npm run lint && cross-env NODE_ENV=production && taro build --type rn",
    "build:qq": "npm run lint && cross-env NODE_ENV=production && taro build --type qq",
    "build:quickapp": "npm run lint && cross-env NODE_ENV=production && taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "start": "npm run dev:weapp",
    "lint": "eslint --fix --ext .js,.jsx src"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "repository": "https://github.com/qjwvtd/taro-app-cli.git",
  "author": "qjwvtd",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.0.16",
    "@tarojs/plugin-platform-alipay": "^3.4.6",
    "@tarojs/plugin-platform-jd": "^3.4.6",
    "@tarojs/plugin-platform-qq": "^3.4.6",
    "@tarojs/plugin-platform-swan": "^3.4.6",
    "@tarojs/plugin-platform-tt": "^3.4.6",
    "@tarojs/plugin-platform-weapp": "^3.4.6",
    "@tarojs/react": "3.0.16",
    "@tarojs/runtime": "3.0.16",
    "@tarojs/taro": "3.0.16",
    "mobx": "^4.8.0",
    "mobx-react": "^6.1.4",
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.0.16",
    "@tarojs/plugin-sass": "^2.2.10",
    "@tarojs/webpack-runner": "3.0.16",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-taro": "3.0.16",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.0.16",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-webpack-plugin": "^2.2.1",
    "mini-css-extract-plugin": "^1.3.1",
    "stylelint": "9.3.0",
    "taro-ui": "^3.0.0-alpha.0"
  }
}
